  },
  "devDependencies": {
    "@types/node": "^20.5.7",
    "typescript": "^5.2.2",
    "vitest": "^4.0.6"
  },
  "dependencies": {
    "@sentry/node": "^8.55.0",
    "encore.dev": "^1.52.2",
    "jose": "^6.1.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.13.0"
  }
}
